<gora-orm>
<table name="Employee" keyClass="java.lang.String" persistentClass="org.gora.example.generated.Employee">
  <fields>
    <field name="name" column="name"/>
    <field name="dateOfBirth" column="dateOfBirth"/>
    <field name="ssn" column="ssn" primarykey="true"/>
    <field name="salary" column="salary"/>
  </fields>
</table>

